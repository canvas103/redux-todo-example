    "babel-preset-es2015-webpack": "^6.4.3",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "cpy-cli": "^1.0.1",
